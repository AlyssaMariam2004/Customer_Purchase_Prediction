[database]
host = your_host
user = your_user
password = your_password
database = your_database

[paths]
csv_filename = NewData1.csv
model_dir = models
model_filename = final_df.pkl
df_filename = df.pkl

[retraining]
interval_seconds = 600
row_growth_threshold = 10

[logging]
log_file = logs/recommendation_system.log
log_level = INFO
log_format = %(asctime)s - %(levelname)s - %(message)s

[model]
top_n = 5
